target 'MOBCore_iOS' do
    use_frameworks!
    platform :ios, '8.0'
    podspec :path => 'MOBCore.podspec'
end

target 'MOBCore_WatchOS' do
    use_frameworks!
    platform :watchos, '2.0'
    podspec :path => 'MOBCore.podspec'
end
